{
  "presets": [["@babel/env", { "modules": false }]],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ],
    "@babel/syntax-dynamic-import",
    "transform-vue-jsx"
  ]
}
